{
  "name": "gitlab-notify",
  "version": "1.3.3",
  "description": "Don't miss any GitLab merge requests and rocket up your productivity.",
  "main": "index.js",
  "scripts": {
    "start": "npm run copy-config:dev && webpack-dev-server --mode development --open --port 5003",
    "build:dev": "npm run copy-config:dev && webpack --mode production",
    "build:prod": "npm run copy-config:prod && webpack --mode production",
    "copy-config:setup": "cp src/config/config.prod.js src/config/config.dev.js",
    "copy-config:dev": "cp src/config/config.dev.js src/config/config.js",
    "copy-config:prod": "cp src/config/config.prod.js src/config/config.js",
    "lint": "tsc --noEmit --rootDir src/ && eslint webpack*.js \"src/**/*.ts{,x}\"",
    "lint:fix": "npm run lint -- --quiet --fix",
    "zip": "yarn neutrino-webextension-zip"
  },
  "author": "Corentin Mors (corentin@pixelswap.fr)",
  "license": "ISC",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ],
  "dependencies": {
    "6d": "^1.0.63",
    "@gitbeaker/browser": "^19.7.0",
    "@primer/components": "^16.3.0",
    "@primer/css": "^14.4.0",
    "@primer/octicons-react": "^9.6.0",
    "async": "^3.2.0"
  },
  "devDependencies": {
    "@neutrinojs/copy": "^9.1.0",
    "@neutrinojs/react": "^9.1.0",
    "@neutrinojs/standardjs": "^9.1.0",
    "@neutrinojs/style-loader": "^9.1.0",
    "@types/async": "^3.2.3",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-compat": "^3.7.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "json-schema": "^0.2.5",
    "neutrino": "^9.1.0",
    "neutrino-typescript": "git+https://github.com/davidje13/neutrino-typescript.git#semver:^1.0.10",
    "neutrino-webextension": "^1.1.8",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "sinon-chrome": "^3.0.1",
    "styled-components": "^5.1.1",
    "typescript": "^3.9.3",
    "webextension-polyfill": "^0.6.0",
    "webextension-polyfill-ts": "^0.14.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
